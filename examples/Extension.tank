class Car{

   func init(name){
      this.name = name;
      print("Init\n");
   }

   func carMethod(){
      print("Method from Car Class\n");
      print(this.name);
   }
}

func Car:move(){
   print("Car name from Extension : " + this.name + "\n");
}

var car = Car("BMW");

car.move();
car.carMethod();